{
  "name": "todorestapi",
  "version": "1.0.0",
  "description": "rest api for todo list using nodejs/expressjs",
  "main": "index.js",
  "scripts": {
    "serve": "node server.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon server.js",
    "dev:lint": "eslint .",
    "test": "IS_TEST=true jest --verbose --maxWorkers=1 --testPathPattern=.test.js",
    "test:watch": "IS_TEST=true jest --watch"
  },
  "author": "calvincoderes",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.0.3",
    "mongoose-timestamp": "^0.6.0",
    "nodemon": "^2.0.22"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "superagent": "^8.0.9",
    "supertest": "^6.3.3"
  }
}
